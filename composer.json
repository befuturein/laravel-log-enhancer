{
    "name": "befuturein/laravel-log-enhancer",
    "description": "A lightweight Laravel package that enriches your logs with structured request, user, and application context.",
    "keywords": [
        "laravel",
        "logging",
        "log",
        "context",
        "observability",
        "monitoring"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "BeFuture Interactive",
            "homepage": "https://befuture.in"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BeFuture\\LogEnhancer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeFuture\\LogEnhancer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeFuture\\LogEnhancer\\LogEnhancerServiceProvider"
            ],
            "aliases": {
                "LogEnhancer": "BeFuture\\LogEnhancer\\Facades\\LogEnhancer"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}